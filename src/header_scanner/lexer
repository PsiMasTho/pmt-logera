
%%
    // ignore comments
#.*\n

    // ignore spaces and tabs on their own
[ \t]+

">attr"					                return HeaderTokens::Tokens_::DECL_ATTR;

">var"                                  return HeaderTokens::Tokens_::DECL_VAR;

[[:print:]]{-}[ \t\n;]+					return HeaderTokens::Tokens_::IDENT;

'\n'                                    return '\n';

.                                    	return matched()[0];
